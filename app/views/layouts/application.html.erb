    <meta name="robots" content="index,follow"/>
    <meta name="revisit-after" content="20 days"/>
    <meta name="distribution" content="global"/>
    <meta name="copyright" content=" <%= t('copyright')%>"/>
    <link rel="canonical" href="http://customgun.com">
    <title>
